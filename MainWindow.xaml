<Window x:Class="Unit5Quadratic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Unit5Quadratic"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Canvas x:Name="canvas">
        <TextBlock Canvas.Left="20" Canvas.Top="38" Height="19" Width="22" RenderTransformOrigin="0.758,0.566">A</TextBlock>
        <TextBlock Canvas.Left="42" Canvas.Top="38">B</TextBlock>
        <TextBlock Canvas.Left="64" Canvas.Top="38">C</TextBlock>
        <TextBox x:Name="TextboxA" Canvas.Left="13" Canvas.Top="58" Height="20" Width="22"/>
        <TextBox x:Name="TextboxB" Canvas.Left="36" Canvas.Top="58" Height="20" Width="22"/>
        <TextBox x:Name="TextboxC" Canvas.Left="58" Canvas.Top="58" Height="20" Width="22" TextChanged="TextboxC_TextChanged"/>
        <Button x:Name="btnComputeClick" Content="Calculate" Click="btnComputeClick_Click" Height="29" Width="67" RenderTransformOrigin="0.222,0.009" Canvas.Left="13" Canvas.Top="78"/>
        <Label x:Name="lblOutput" Canvas.Left="48" Canvas.Top="5"/>
    </Canvas>
</Window>
